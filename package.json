{
  "name": "movie-web",
  "version": "1.0.0",
  "description": "A movie streaming web application with PostgreSQL backend and React frontend",
  "main": "backend/bin/www",
  "scripts": {
    "start": "node backend/bin/www",
    "server": "cd backend && npm start",
    "client": "cd client && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd backend && npm install && cd ../client && npm install"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "multer": "^2.0.2",
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "movie",
    "streaming",
    "react",
    "postgresql",
    "express",
    "nodejs"
  ],
  "author": "Your Name",
  "license": "MIT"
}
